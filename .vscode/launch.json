{
  "configurations": [

      {
          "name": "Service",
          "runtimeVersion": "16.0.0",
          "preLaunchTask": "npm: build", // npm run build
          "runtimeArgs": ["start"], // npm run start
          "request": "launch",
          "runtimeExecutable": "npm",
          "type": "node",
          "skipFiles": ["<node_internals>/**"],
      },
      {
          "name": "Watch",
          "runtimeVersion": "16.0.0",
          "runtimeArgs": ["start", "dev"], // npm run start:dev
          "request": "launch",
          "runtimeExecutable": "npm",
          "type": "node",
          "skipFiles": ["<node_internals>/**"],
      }
  ]
}